{
	"name": "jkuchar/pdfresponse",
	"description": "PdfResponse is wrapper of mPDF for Nette.",
	"license": "LGPL-2.0-or-later",
	"scripts": {
		"verify": [
			"@phpstan",
			"@tester"
		],
		"tester": "vendor/bin/tester test",
		"phpstan": "vendor/bin/phpstan analyze src -c phpstan.neon --memory-limit 1G -l 1 --error-format compact"
	},
	"authors": [
		{
			"name": "Jan KuchaÅ™",
			"homepage": "http://www.jankuchar.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"PdfResponse\\": "src"
		}
	},
	"require": {
		"php": ">= 7.1.0",
		"nette/utils": ">= 2.4",
		"nette/http": ">= 2.4",
		"nette/application": ">= 2.4",
		"mpdf/mpdf": ">=7.0.0",
		"paquettg/php-html-parser": "^2.0"
	},
	"require-dev": {
		"nette/tester": "^2.2",
		"phpstan/phpstan": "^0.11.12",
		"grifart/phpstan-oneline": "^0.2.2"
	}
}
