{
	"name": "jkuchar/pdfresponse",
	"description": "PdfResponse is wrapper of mPDF for Nette.",
	"license": "LGPL-2.0-or-later",
	"scripts": {
		"verify": [
			"@phpstan",
			"@tester"
		],
		"tester": "vendor/bin/tester test",
		"phpstan": "vendor/bin/phpstan analyze src -c phpstan.neon --memory-limit 1G -l 1 --error-format compact"
	},
	"authors": [
		{
			"name": "Jan Kucha≈ô",
			"homepage": "http://www.jankuchar.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"PdfResponse\\": "src"
		}
	},
	"require": {
		"php": "8.3 - 8.5",
		"nette/utils": "^4.0",
		"nette/http": "^3.2",
		"nette/application": "^3.1",
		"mpdf/mpdf": "^8.0",
		"paquettg/php-html-parser": "^3.1"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan": "^2.1"
	}
}
